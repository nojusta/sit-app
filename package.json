    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "appwrite": "^16.0.2",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "metro": "^0.81.0",
    "nativewind": "^2.0.11",
    "react-native-animatable": "^1.4.0",
    "react-native-appwrite": "^0.5.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.3.3"
  },
  "private": true
