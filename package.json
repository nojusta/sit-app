    "metro": "~0.80.8",
    "nativewind": "^2.0.11",
    "react-native-animatable": "^1.4.0",
    "react-native-appwrite": "^0.5.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.3.3"
