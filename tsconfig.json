{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "nativewind/types"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "lib/appwrite.js",
    "@types",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
